@using Storm.Formification.Core
@using Storm.Formification.Web.Controllers

@model FormModel

<div class="col-md-8 col-lg-6 form-horziontal">
    <form method="post" enctype="multipart/form-data" asp-antiforgery="true" asp-route-id="@Model.Id">
        @Html.RenderForm(m => m.BankAccount)
        @Html.RenderForm(m => m.Pet)
        <hr />
        <div class="form-row mt-3">
            <input type="submit" value="Submit" />
        </div>
    </form>
</div>

@section scripts
{
    @await Html.PartialAsync("_ValidationScriptsPartial")
}